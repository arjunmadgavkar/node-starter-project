{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    }, 
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "rules": {
        "no-console": "off",
        "no-debugger": "warn",
        "prettier/prettier": "warn"
    }
}